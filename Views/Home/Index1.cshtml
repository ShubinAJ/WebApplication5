@model IEnumerable<WebApplication5.Models.Product>

<h2>Список пользователей</h2>
<p><a asp-action="Create" asp-controller="Home">Добавить пользователя</a></p>

<table class="table">
    <tr><th>Товар</th><th>Цена</th><th>Адрес Url</th><th>Дата добавления</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            @*<td>@item.Price</td>*@
            <td>@item.Url</td>
            @*<td>@item.Date</td>*@
            <td>
                <a asp-action="Edit" asp-controller="Home" asp-route-id="@item.Id">Изменить</a>
            </td>
            <td>
                <form asp-controller="Home" asp-action="Delete" method="post" asp-route-id="@item.Id">
                    <input type="submit" value="Удалить" />
                </form>
            </td>
        </tr>
    }
</table>